{
  "name": "wallet-ui",
  "version": "0.2.23",
  "description": "Equibit Web Wallet UI",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EDC/wallet-ui.git"
  },
  "author": {
    "name": "EDC",
    "email": "info@equibit.org",
    "url": "https://equibit.org"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "node start.js",
    "develop": "done-serve --static --develop --port 8080",
    "build": "npm run remove-build && node build",
    "remove-build": "make remove-build",
    "docs": "documentjs -f -d",
    "docs:publish": "npm run build && npm run docs && make docs-publish",
    "lint": "semistandard",
    "lint:fix": "semistandard --fix",
    "test": "npm-run-all lint test:unit",
    "test:funcunit": "testee test/test-funcunit.html --browsers firefox --reporter Spec",
    "test:unit": "testee test/test.html --browsers firefox --reporter Spec",
    "qa": "npm run release:qa:patch",
    "release:qa:patch": "npm test && npm run build && npm version patch && make release-qa",
    "release:qa:minor": "npm test && npm run build && npm version minor && make release-qa",
    "release:qa:major": "npm test && npm run build && npm version major && make release-qa",
    "deps-bundle": "steal-tools bundle --deps"
  },
  "main": "wallet-ui/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [
    "equibit",
    "bitcoin",
    "keychain",
    "blockchain",
    "wallet"
  ],
  "semistandard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "dev-bundle.js"
    ]
  },
  "steal": {
    "main": "wallet-ui/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "plugins": [
      "chai",
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "/dist"
      }
    },
    "serviceBaseURL": "",
    "map": {
      "assert": "chai/chai"
    },
    "meta": {
      "bootstrap/dist/js/bootstrap": {
        "deps": [
          "jquery"
        ]
      },
      "chai/chai": {
        "format": "global",
        "exports": "chai.assert"
      },
      "tether": {
        "exports": "Tether"
      }
    }
  },
  "dependencies": {
    "@equibit/wallet-crypto": "^0.1.0",
    "accounting": "^0.4.1",
    "bit-c3": "^0.2.3",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "bootstrap-canjs": "^0.1.4",
    "can-cid": "^1.0.3",
    "can-component": "^3.0.7",
    "can-connect": "^1.3.10",
    "can-connect-feathers": "^3.5.2",
    "can-define": "^1.0.20",
    "can-define-stream": "^0.0.7",
    "can-event": "^3.3.0",
    "can-route": "^3.0.10",
    "can-route-pushstate": "^3.0.1",
    "can-search-select": "^0.2.4",
    "can-set": "^1.2.0",
    "can-stache": "^3.0.24",
    "can-stream": "^0.0.5",
    "can-view-autorender": "^3.0.3",
    "can-zone": "^0.6.0",
    "candlestick-volume-chart": "0.1.18",
    "copy-to-clipboard": "^3.0.6",
    "core-js": "^2.4.1",
    "done-autorender": "^1.0.0-alpha.0",
    "done-component": "^1.0.0",
    "done-css": "^3.0.0",
    "done-serve": "^1.2.0",
    "express-session": "^1.15.2",
    "feathers": "^2.1.1",
    "feathers-authentication-client": "^0.3.1",
    "feathers-authentication-signed": "^2.0.3",
    "feathers-hooks": "^2.0.0",
    "feathers-rest": "^1.7.2",
    "feathers-socketio": "^1.6.0",
    "flexboxgrid-less": "^0.0.1",
    "generator-donejs": "^1.0.4",
    "grid-component": "^0.9.0",
    "jquery": "^3.2.1",
    "kefir": "^3.7.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "tether": "^1.4.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "can-fixture": "^1.0.11",
    "can-view-model": "^3.1.3",
    "chai": "^3.5.0",
    "documentjs": "^0.4.4",
    "donejs-cli": "^1.0.1",
    "funcunit": "^3.3.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "semistandard": "^10.0.0",
    "steal": "^1.5.1",
    "steal-less": "^1.2.0",
    "steal-mocha": "^1.0.0",
    "steal-socket.io": "^4.0.9",
    "steal-stache": "^3.0.5",
    "steal-tools": "^1.3.3",
    "testee": "^0.4.0"
  }
}
