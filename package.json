{
  "name": "wallet-ui",
  "version": "0.0.1",
  "description": "Equibit Web Wallet UI",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EDC/wallet-ui.git"
  },
  "author": {
    "name": "EDC",
    "email": "info@equibit.org",
    "url": "https://equibit.org"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "done-serve --port 8080",
    "develop": "done-serve --static --develop --port 8080",
    "build": "node build",
    "document": "documentjs -f -d",
    "lint": "eslint src test --ext .js --ext .component",
    "test": "npm-run-all lint test:unit",
    "test:unit": "testee test.html --browsers firefox --reporter Spec"
  },
  "main": "wallet-ui/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [
    "equibit",
    "bitcoin",
    "keychain",
    "wallet"
  ],
  "steal": {
    "main": "wallet-ui/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "/dist"
      }
    },
    "serviceBaseURL": ""
  },
  "dependencies": {
    "can-component": "^3.0.4",
    "can-connect": "^1.0.21",
    "can-define": "^1.0.10",
    "can-route": "^3.0.6",
    "can-route-pushstate": "^3.0.1",
    "can-set": "^1.0.3",
    "can-stache": "^3.0.16",
    "can-view-autorender": "^3.0.3",
    "can-zone": "^0.6.0",
    "done-serve": "^1.0.0-alpha.0",
    "done-autorender": "^1.0.0-alpha.0",
    "done-component": "^0.6.0-pre.5",
    "done-css": "^3.0.0-alpha.0",
    "generator-donejs": "^1.0.0-alpha.0",
    "steal": "^1.0.5",
    "steal-less": "^1.0.2",
    "steal-stache": "^3.0.4"
  },
  "devDependencies": {
    "can-fixture": "^1.0.11",
    "documentjs": "^0.4.4",
    "donejs-cli": "^1.0.0-alpha.2",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-done-component": "^0.2.3",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.4.1",
    "funcunit": "^3.1.0",
    "npm-run-all": "^4.0.2",
    "steal-qunit": "^1.0.0",
    "steal-tools": "^1.0.1",
    "testee": "^0.3.0"
  }
}
