{
  "name": "wallet-ui",
  "version": "0.0.1",
  "description": "Equibit Web Wallet UI",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EDC/wallet-ui.git"
  },
  "author": {
    "name": "EDC",
    "email": "info@equibit.org",
    "url": "https://equibit.org"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "done-serve --port 8080",
    "develop": "done-serve --static --develop --port 8080",
    "build": "node build",
    "document": "documentjs -f -d",
    "lint": "semistandard",
    "lint:fix": "semistandard --fix",
    "test": "npm-run-all lint test:unit",
    "test:funcunit": "testee test/test-funcunit.html --browsers firefox --reporter Spec",
    "test:unit": "testee test/test.html --browsers firefox --reporter Spec"
  },
  "main": "wallet-ui/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [
    "equibit",
    "bitcoin",
    "keychain",
    "wallet"
  ],
  "semistandard": {
    "env": [
      "mocha"
    ]
  },
  "steal": {
    "main": "wallet-ui/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "plugins": [
      "chai",
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "/dist"
      }
    },
    "serviceBaseURL": "",
    "map": {
      "assert": "chai/chai"
    },
    "meta": {
      "bootstrap": {
        "deps": [
          "jquery",
          "tether"
        ]
      },
      "chai/chai": {
        "format": "global",
        "exports": "chai.assert"
      },
      "tether": {
        "exports": "Tether"
      }
    }
  },
  "dependencies": {
    "@equibit/wallet-crypto": "^0.1.0",
    "bootstrap": "^3.3.7",
    "can-component": "^3.0.7",
    "can-connect": "^1.0.21",
    "can-connect-feathers": "^3.5.2",
    "can-define": "^1.0.10",
    "can-route": "^3.0.6",
    "can-route-pushstate": "^3.0.1",
    "can-set": "^1.0.3",
    "can-stache": "^3.0.21",
    "can-view-autorender": "^3.0.3",
    "can-zone": "^0.6.0",
    "done-autorender": "^1.0.0-alpha.0",
    "done-component": "^1.0.0",
    "done-css": "^3.0.0",
    "done-serve": "^1.0.0-alpha.0",
    "feathers": "^2.1.1",
    "feathers-authentication-client": "^0.3.1",
    "feathers-authentication-signed": "^0.0.4",
    "feathers-hooks": "^1.8.1",
    "feathers-rest": "^1.7.1",
    "feathers-socketio": "^1.5.2",
    "flexboxgrid": "^6.3.1",
    "generator-donejs": "^1.0.2",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "steal": "^1.3.2",
    "steal-socket.io": "^4.0.9",
    "tether": "^1.4.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "can-fixture": "^1.0.11",
    "chai": "^3.5.0",
    "documentjs": "^0.4.4",
    "donejs-cli": "^1.0.1",
    "funcunit": "^3.1.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "semistandard": "^10.0.0",
    "steal-less": "^1.2.0",
    "steal-mocha": "^1.0.0",
    "steal-stache": "^3.0.5",
    "steal-tools": "^1.0.1",
    "testee": "^0.3.0"
  }
}
