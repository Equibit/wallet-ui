<can-import from="~/utils/stache-helpers/"></can-import>
<can-import from="bit-c3" />

<h2 class="sub-title">{{i18n('breakdown')}}</h2>

<div class="well">
  <div class="row">
    <bit-c3 {config}="config">
      <bit-c3-data type="donut">
        {{#each dataColumns}}
          <bit-c3-data-column value="{.}" />
        {{/each}}
      </bit-c3-data>
      <c3-donut title="{title}" />
    </bit-c3>
  </div>
</div>