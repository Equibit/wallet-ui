{{#if currency.symbol}}

  <div class="input-group quantity-usd">
    <input type="text" class="form-control" {($value)}="outputFormatted" aria-describedby="quantity-{{currency.symbol}}">
    <span class="input-group-addon" id="quantity-{{currency.symbol}}">{{currency.symbol}}</span>
  </div>

{{else}}

  <input type="text" {($value)}="outputFormatted" />\

{{/if}}

